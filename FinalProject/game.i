void updatePlayer() {

    if((~((*(volatile unsigned short *)0x04000130)) & ((1<<6)))) {
        if (person.worldRow > 0 && collisionMapBitmap[(((person.worldRow - person.rdel))*(256)+(person.worldCol))]
         && collisionMapBitmap[(((person.worldRow - person.rdel))*(256)+((person.worldCol + person.width - person.cdel)))]) {

         person.worldRow--;
            if (vOff > 0 && person.screenRow < 160 / 2) {

          vOff--;
            } else {
                vFOff++;
            }
        }
    }
    if((~((*(volatile unsigned short *)0x04000130)) & ((1<<7)))) {
        if (person.worldRow + person.height < 256 && collisionMapBitmap[(((person.worldRow + person.height))*(256)+(person.worldCol))]
         && collisionMapBitmap[(((person.worldRow + person.height))*(256)+((person.worldCol + person.width - person.cdel)))]) {

         person.worldRow++;
            if (vOff < 96 && person.screenRow > 160 / 2) {

          vOff++;
            } else {
                vFOff--;
            }
        }
    }
    if((~((*(volatile unsigned short *)0x04000130)) & ((1<<5)))) {
        if (person.worldCol > 0 && collisionMapBitmap[((person.worldRow)*(256)+((person.worldCol - person.cdel)))]
        && collisionMapBitmap[(((person.worldRow + person.height - person.rdel))*(256)+((person.worldCol)))]) {

   person.worldCol--;
            if (hOff > 0 && person.screenCol < 240 / 2) {

             hOff--;
            } else {
                hFOff++;
            }
        }
    }
    if((~((*(volatile unsigned short *)0x04000130)) & ((1<<4)))) {
        if (person.worldCol + person.width < 256 && collisionMapBitmap[((person.worldRow)*(256)+((person.worldCol + person.width + 1)))]
        && collisionMapBitmap[(((person.worldRow + person.height - person.rdel))*(256)+((person.worldCol + person.width)))]) {

            person.worldCol++;
            if (hOff < 16 && person.screenCol > 240 / 2) {

                hOff++;
            } else {
                hFOff--;
            }
        }
    }

    person.screenRow = person.worldRow - vOff;
    person.screenCol = person.worldCol - hOff;

    animatePlayer();
    testCollisions();
}


void animatePlayer() {


    person.prevAniState = person.aniState;
    person.aniState = PERSONIDLE;


    if(person.aniCounter % 20 == 0) {
        person.curFrame = (person.curFrame + 1) % person.numFrames;
    }


    if((~((*(volatile unsigned short *)0x04000130)) & ((1<<6))))
        person.aniState = PERSONBACK;
    if((~((*(volatile unsigned short *)0x04000130)) & ((1<<7))))
        person.aniState = PERSONFRONT;
    if((~((*(volatile unsigned short *)0x04000130)) & ((1<<5))))
        person.aniState = PERSONLEFT;
    if((~((*(volatile unsigned short *)0x04000130)) & ((1<<4))))
        person.aniState = PERSONRIGHT;


    if (person.aniState == PERSONIDLE) {
        person.curFrame = 0;
        person.aniCounter = 0;
        person.aniState = person.prevAniState;
    } else {
        person.aniCounter++;
    }
}


void testCollisions() {

    if (can.active && collision(person.screenRow, person.screenCol, person.height, person.width, (can.row - vOff), (can.col - hOff), can.height, can.width)) {
