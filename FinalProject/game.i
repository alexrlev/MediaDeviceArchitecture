        can.active = 0;
        can.timer = 0;
        playSoundB(chaChing, 8773, 11025, 0);
    }
    if (apple.active && collision(person.screenRow, person.screenCol, person.height, person.width, (apple.row - vOff), (apple.col - hOff), apple.height, apple.width)) {
        moneyCounter -= foodPrice;
        foodCounter++;
        apple.active = 0;
